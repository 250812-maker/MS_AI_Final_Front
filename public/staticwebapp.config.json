{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "mimeTypes": {
    ".js": "application/javascript",
    ".mjs": "application/javascript"
  },
  "routes": [
    {
      "route": "/assets/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*",
      "headers": {
        "cache-control": "no-store, no-cache, must-revalidate"
      }
    }
  ]
}
